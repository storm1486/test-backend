{
  "author": "Jaco Greeff <jacogr@gmail.com>",
  "bugs": "https://github.com/polkadot-js/wasm/issues",
  "contributors": [],
  "description": "Wasm content for wasm-crypto",
  "engines": {
    "node": ">=14.0.0"
  },
  "homepage": "https://github.com/polkadot-js/wasm/tree/master/packages/wasm-crypto-wasm#readme",
  "license": "Apache-2.0",
  "maintainers": [],
  "name": "@polkadot/wasm-crypto-wasm",
  "repository": {
    "directory": "packages/wasm-crypto-wasm",
    "type": "git",
    "url": "https://github.com/polkadot-js/wasm.git"
  },
  "sideEffects": false,
  "type": "module",
  "version": "4.6.1",
  "main": "data.cjs",
  "module": "data.js",
  "browser": "data.js",
  "react-native": "data.js",
  "types": "data.d.ts",
  "exports": {
    ".": {
      "types": "./data.d.ts",
      "react-native": {
        "require": "./data.cjs",
        "default": "./data.js"
      },
      "browser": {
        "require": "./data.cjs",
        "default": "./data.js"
      },
      "node": {
        "require": "./data.cjs",
        "default": "./data.js"
      }
    },
    "./base64": {
      "types": "./base64.d.ts",
      "require": "./base64.cjs",
      "default": "./base64.js"
    },
    "./bytes": {
      "types": "./bytes.d.ts",
      "require": "./bytes.cjs",
      "default": "./bytes.js"
    },
    "./cjs/bytes": {
      "types": "./cjs/bytes.d.ts",
      "default": "./cjs/bytes.js"
    },
    "./cjs/package.json": "./cjs/package.json",
    "./data": {
      "types": "./data.d.ts",
      "require": "./data.cjs",
      "default": "./data.js"
    },
    "./detectOther": {
      "types": "./detectOther.d.ts",
      "require": "./detectOther.cjs",
      "default": "./detectOther.js"
    },
    "./detectPackage": {
      "types": "./detectPackage.d.ts",
      "require": "./detectPackage.cjs",
      "default": "./detectPackage.js"
    },
    "./empty": {
      "types": "./empty.d.ts",
      "require": "./empty.cjs",
      "default": "./empty.js"
    },
    "./fflate": {
      "types": "./fflate.d.ts",
      "require": "./fflate.cjs",
      "default": "./fflate.js"
    },
    "./package.json": "./package.json",
    "./packageInfo.cjs": "./packageInfo.cjs",
    "./packageInfo.js": "./packageInfo.js",
    "./packageInfo": {
      "types": "./packageInfo.d.ts",
      "require": "./packageInfo.cjs",
      "default": "./packageInfo.js"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.17.2"
  },
  "peerDependencies": {
    "@polkadot/util": "*"
  }
}